    "import pandas as pd\n",
    "\n",
    "# Load the generated CSV file\n",
    "output_df = pd.read_csv('gpt2-out.csv')\n",
    "\n",
    "# Replace occurrences of '<|endoftext|>' with an empty string across all columns\n",
    "output_df.replace(r\"<\\|endoftext\\|>\", \"\", regex=True, inplace=True)\n",
    "\n",
    "# Save the cleaned data back to a new CSV file\n",
    "output_df.to_csv('gpt2-out.csv')\n",
    "\n",
    "print(\"Pruned CSV file 'gpt2-out-pruned.csv' created successfully.\")\n"
   ]
