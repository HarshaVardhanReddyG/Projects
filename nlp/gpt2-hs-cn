    "#     model=model,\n",
    "#     args=training_args,\n",
    "#     train_dataset=train_dataset,\n",
    "#     eval_dataset=valid_dataset\n",
    "# )\n",
    "\n",
    "# # Train the model\n",
    "# trainer.train()\n",
